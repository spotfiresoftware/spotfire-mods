{
    "apiVersion": "1.0",
    "version": "1.0",
    "name": "Gantt chart",
    "id": "one-hour-gantt",
    "icon": "icon.svg",
    "properties": [],
    "dataViewDefinition": {
        "axes": [
            {
                "name": "Task",
                "mode": "categorical",
                "placement": "none",
                "dropTarget": {
                    "icon": "XAxis3D",
                    "description": "Use {0} on the mod's Task-axis"
                }
            },
            {
                "name": "Link",
                "mode": "categorical",
                "placement": "none",
                "dropTarget": {
                    "icon": "XAxis3D",
                    "description": "Use {0} on the mod's Task-axis"
                }
            },
            {
                "name": "start",
