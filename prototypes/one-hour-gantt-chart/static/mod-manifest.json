                "dataTypes": {
                    "allowBoolean": false,
                    "allowDateTime": true,
                    "allowNumeric": false,
                    "allowString": false,
                    "allowTimeSpan": false
                },
                "placement": "none"
            },
            {
                "name": "percent",
                "mode": "continuous",
                "dataTypes": {
                    "allowBoolean": false,
                    "allowDateTime": false,
                    "allowNumeric": true,
                    "allowString": false,
                    "allowTimeSpan": false
                },
                "placement": "none"
            }
        ]
    },
    "files": [
        "index.html",
        "main.css",
        "bundle.js"
