{
  "apiVersion": "$MOD-API-VERSION",
  "version": "1.0.0",
  "name": "ECharts Graph",
  "id": "examples.ts-echarts-graph",
  "icon": "icon.svg",
  "type": "visualization",
  "properties": [
    {
      "name": "layout",
      "type": "string",
      "defaultValue": "force"
    },
    {
      "name": "labelOverlap",
      "type": "boolean",
      "defaultValue": true
    }
  ],
  "dataViewDefinition": {
    "colorAxis": {
      "mode": "dual",
      "dropTarget": {
        "icon": "Color",
        "description": "Color by {0}"
      }
    },
    "axes": [
      {
        "name": "Node",
        "mode": "categorical",
        "dropTarget": {
          "icon": "Nodes",
          "description": "Nodes (id/label)"
        }
      },
      {
        "name": "Link",
        "mode": "categorical",
        "dropTarget": {
          "icon": "Link",
          "description": "Links (source,target)"
        }
      },
      {
        "name": "Size",
        "mode": "continuous",
        "dropTarget": {
          "icon": "Size",
          "description": "Size by {0}"
        }
      },
      {
        "name": "X",
        "mode": "continuous",
        "dropTarget": {
          "icon": "X",
          "description": "X position"
        }
      },
      {
        "name": "Y",
        "mode": "continuous",
        "dropTarget": {
          "icon": "Y",
          "description": "Y position"
        }
      }
    ]
  },
  "files": ["index.html", "main.css", "build/main.js"]
}