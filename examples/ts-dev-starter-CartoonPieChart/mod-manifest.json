{
    "apiVersion": "1.2",
    "version": "1.0",
    "name": "Cartoon Pie Chart",
    "id": "cartoon-pie-chart",
    "icon": "icon.svg",
    "properties": [
        {
            "name": "myProperty",
            "type": "string",
            "defaultValue": "myValue"
        }
    ],
    "dataViewDefinition": {
        "colorAxis": {
            "mode": "dual",
            "dropTarget": {
                "icon": "Color",
                "description": "Color by {0}"
            }
        },
        "axes": [
            {
                "name": "X",
                "mode": "categorical",
                "placement": "bottom",
                "dropTarget": {
                    "icon": "Category",
                    "description": "Use {0} for pie slice labels"
                }
            },
            {
                "name": "Y",
                "mode": "continuous",
                "placement": "left",
                "dropTarget": {
                    "icon": "YAxis3D",
                    "description": "Use {0} for pie slice values"
                }
            }
        ]
    },
    "files": [
        "index.html",
        "main.css",
        "build/main.js"
    ]
}
